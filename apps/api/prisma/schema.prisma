generator client {
  provider = "prisma-client-js"
  // Pastikan output ini sesuai dengan import di PrismaService kamu
  output   = "../src/generated/prisma" 
}

datasource db {
  provider = "postgresql"
}

model AnalysisCache {
  id          String   @id @default(cuid())
  inputHash   String   @unique
  resumeText  String
  jobDescText String
  resultJson  Json
  createdAt   DateTime @default(now())
}